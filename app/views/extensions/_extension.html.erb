<%= render :partial => 'thead', :locals => {:extension => extension} %>
<tbody class="extension-<%= extension.id %>">
  <% extension.updates.each do |update| %>
    <tr>
      <td></td>
      <td class="text-center"><%= link_to update.version, [extension, update]  %></td>
      <td class="text-center"><%= update.date.strftime("%-m/%-d/%Y")  %></td>
      <%= form_with model: [extension, update] do |form| %>
        <td class="text-center"><%= form.check_box :reviewed, onchange: 'Rails.fire(this.form, "submit")'  %></td>      
      <% end %>
      <td class="edit-cell"></td>
      <td class="delete-cell"></td>
    </tr>
  <% end %>
</tbody>