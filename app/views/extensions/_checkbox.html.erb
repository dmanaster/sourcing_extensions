      <% if update.reviewed == true %>
        <%= link_to '<i class="far fa-check-square"></i>'.html_safe, extension_update_path(update.extension, update, :update => { :reviewed => false }), :method => :put, :remote => true %>
      <% else %>
        <%= link_to '<i class="far fa-square"></i>'.html_safe, extension_update_path(update.extension, update, :update => { :reviewed => true }), :method => :put, :remote => true %>
      <% end %>